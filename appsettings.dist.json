/* Copy this file to appsettings.json in the repo root, and enter valid credentials and URLs */
{
  "PDFinch": {
    /* Either configure one client */
    "Name": "InvoiceClient",
    "Environment": "Production",
    "ApiKey": "your-production-key-here",
    "ApiSecret": "your-production-secret-here",

    /* Or multiple */
    "Clients": [
      {
        "Name": "ShippingLabelClient",
        "Environment": "Staging",
        "ApiKey": "your-staging-key-here",
        "ApiSecret": "your-staging-secret-here"
      },
      {
        "Name": "DebugClient",
        "Environment": "Custom",
        "BaseUrl": "https://localhost:44329/",
        "ApiKey": "your-development-key-here",
        "ApiSecret": "your-development-secret-here"
      }
    ]
  }
}
